<Window x:Class="Bigarren_ariketa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bigarren_ariketa"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="resultado" HorizontalAlignment="Left" Margin="236,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="304" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="frase1" Content="Frase 1" HorizontalAlignment="Left" Margin="236,163,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="True"/>
        <Button x:Name="frase2" Content="Frase 2" HorizontalAlignment="Left" Margin="355,163,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="False"/>
        <Button x:Name="frase3" Content="Frase 3" HorizontalAlignment="Left" Margin="472,163,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="False"/>
        <Button x:Name="frase4" Content="Frase 4" HorizontalAlignment="Left" Margin="236,217,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="False"/>
        <Button x:Name="frase5" Content="Frase 5" HorizontalAlignment="Left" Margin="355,217,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="False"/>
        <Button x:Name="unir" Content="Unir" HorizontalAlignment="Left" Margin="472,217,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="Button_Click" Width="68" IsEnabled="False">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0"/>
                    <RotateTransform/>
                    <TranslateTransform X="0"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="limpiar" Content="Limpiar" HorizontalAlignment="Left" Margin="236,277,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="limpieza" Width="304" IsEnabled="True"/>
        <Button x:Name="salir" Content="Salir" HorizontalAlignment="Left" Margin="236,314,0,0" VerticalAlignment="Top" RenderTransformOrigin="-5.457,-2.623" Click="cerrar" Width="304" IsEnabled="True"/>

    </Grid>
</Window>
